{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n// @ is an alias to /src\nimport plantCard from \"@/components/PlantCard.vue\";\nimport filterList from \"@/components/FilterList.vue\";\nimport Vue from \"vue\";\nexport default {\n  name: \"PlantView\",\n  components: {\n    plantCard: plantCard,\n    filterList: filterList\n  },\n  data: function data() {\n    return {\n      plants: Vue.prototype.$selectedPlants,\n      selected_tags: []\n    };\n  },\n  methods: {\n    ShowDetail: function ShowDetail(plantId) {\n      this.$router.push(\"/plant/\".concat(plantId));\n    },\n    updatePlants: function updatePlants(p1) {\n      this.plants = p1; // console.log(\"Plants: \", p1);\n    },\n    updateTags: function updateTags(t1) {\n      console.log(\"T1\", t1);\n      this.selected_tags = t1;\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkDA;AACA;AACA;AACA;AACA;AACAA,mBADA;AAEAC;AACAC,wBADA;AAEAC;AAFA,GAFA;AAMAC;AACA;AACAC,2CADA;AAEAC;AAFA;AAIA,GAXA;AAYAC;AACAC,cADA,sBACAC,OADA,EACA;AACA;AACA,KAHA;AAIAC,gBAJA,wBAIAC,EAJA,EAIA;AACA,uBADA,CAEA;AACA,KAPA;AAQAC,cARA,sBAQAC,EARA,EAQA;AACAC;AACA;AACA;AAXA;AAZA","names":["name","components","plantCard","filterList","data","plants","selected_tags","methods","ShowDetail","plantId","updatePlants","p1","updateTags","t1","console"],"sourceRoot":"src/views","sources":["PlantView.vue"],"sourcesContent":["<template>\n  <div class=\"plant\">\n     <div  class=\"header\">\n       <!-- Header for sidebar existing -->\n        <h2 class=\"planttitle\">Plant Catalog</h2>\n        <!-- Header for bottom navigation bar existing -->\n        <h2 class=\"sm_planttitle\">Playful Plants</h2>\n        <div class=\"inp-box\">\n          <b-form-input\n            type=\"text\"\n            class=\"form-control\"\n            placeholder=\"Search\"\n            readonly\n          ></b-form-input>\n          <b-button type=\"submit\" variant=\"success\"\n          class=\"search-btn\">Search</b-button>\n          <filterList v-on:update-plantlist=\"updatePlants\"\n                          :selected-tags=\"updateTags\" class=\"updateplants\"/>\n        </div>\n        <div v-if=\"selected_tags != []\" class=\"filterTags\">\n          <h4>Selected Plant Tags: </h4>\n          <b-button\n            v-for=\"(tag, number) in selected_tags\"\n            v-bind:key=\"number\"\n            variant=\"outline-dark\"\n            id=\"plantTag\"\n            disabled\n            class=\"tags\"\n            >{{ tag }}\n          </b-button>\n        </div>\n     </div>\n    <b-container fluid class=\"main-view\">\n      <b-row>\n        <div class=\"waiBox\">\n          <div class=\"scroller\">\n            <plantCard\n              v-for=\"value in plants\"\n              v-bind:plant_id=\"value\"\n              v-on:click.native=\"ShowDetail(value)\"\n              v-bind:key=\"value.id\"\n            />\n          </div>\n        </div>\n      </b-row>\n    </b-container>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport plantCard from \"@/components/PlantCard.vue\";\nimport filterList from \"@/components/FilterList.vue\";\nimport Vue from \"vue\";\nexport default {\n  name: \"PlantView\",\n  components: {\n    plantCard,\n    filterList,\n  },\n  data: function () {\n    return {\n      plants: Vue.prototype.$selectedPlants,\n      selected_tags: []\n    };\n  },\n  methods: {\n    ShowDetail(plantId) {\n      this.$router.push(`/plant/${plantId}`);\n    },\n    updatePlants(p1) {\n      this.plants = p1;\n      // console.log(\"Plants: \", p1);\n    },\n    updateTags(t1) {\n      console.log(\"T1\", t1);\n      this.selected_tags = t1;\n    }\n  },\n};\n</script>\n\n<style scoped>\n.header{\n  position: fixed;\n  z-index: 99;\n  background: #fff;\n  padding-bottom: 20px;\n  top:0;\n}\n.inp-box{\n  display: flex;\n  justify-content: center;\n}\n.search-btn{\n  margin-left: 10px;\n  flex:1;\n}\n/* set layout for tablets, large tablets, and desktops*/\n@media only screen and (min-width:600px) {\n  .plant {\n    margin-left: 200px;\n  }\n  .header{\n    width: calc(100% - 200px);\n  }\n  .inp-box{\n    padding: 0 15px;\n  }\n  .planttitle{\n    display: flex;\n    margin-top: 20px;\n    margin-bottom: 20px;\n    justify-content: center;\n  }\n  .sm_planttitle{\n    display: none;\n  }\n}\n/* set layout for phones */\n@media only screen and (max-width:600px) {\n  .plant{\n    width: 100%;\n    min-width: 360px;\n    margin-bottom:100px\n  }\n  .header{\n    width: 100%;\n  }\n  .inp-box{\n    padding: 0 5px;\n  }\n  .search-btn{\n    margin-left: 5px;\n  }\n  .planttitle{\n    display: none;\n  }\n  .sm_planttitle{\n    display: flex;\n    margin-top: 20px;\n    margin-bottom: 20px;\n    justify-content: center;\n  }\n}\n@media screen and (max-width: 960px){\n  .scroller {\n    position: relative;\n    column-count: 2 !important;\n    column-gap: 20px;\n  }\n}\n\n@media screen and (max-width: 768px){\n  .scroller {\n    position: relative;\n    column-count: 1 !important;\n    column-gap: 20px;\n  }\n}\n\n.main-view {\n  width: 100%;\n  min-width: 300px;\n  margin-top: 131px;\n}\n.scroller {\n  position: relative;\n  column-count: 3;\n  column-gap: 20px;\n  text-align: left;\n}\n.searchButton {\n  margin-left: 0px;\n}\n\n.form-control {\n  flex:6;\n}\n\n.updateplants {\n  flex:1;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}