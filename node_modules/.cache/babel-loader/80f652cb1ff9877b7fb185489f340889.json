e.log(dataUrl);\n        self.addToPhotoGallery(dataUrl); // self.uploadPhoto(dataUrl);\n      }, FLASH_TIMEOUT);\n    },\n    addToPhotoGallery: function addToPhotoGallery(dataURI) {\n      console.log(\"add to photo gallery\");\n      this.items = [];\n      this.items.push({\n        src: dataURI,\n        thumbnail: dataURI,\n        w: this.canvasWidth / 2,\n        h: this.canvasHeight / 2,\n        alt: \"\"\n      });\n    },\n    returnAndSave: function returnAndSave() {\n      this.stopCameraStream();\n\n      if (this.items.length != 0) {\n        Vue.prototype.$photo = this.items[0].src;\n      }\n\n      this.$router.push(\"/journal/new\");\n    },\n    close: function close() {\n      this.stopCameraStream();\n      this.$router.push(\"/journal/new\");\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmDA;AACA;AAEA;AACAA,mBADA;AAEAC;AACAC;AADA,GAFA;AAKAC,MALA,kBAKA;AACA;AACAC,wBADA;AAEAC,uBAFA;AAGAC,sBAHA;AAIAC;AAJA;AAMA,GAZA;AAaAC,SAbA,qBAaA;AACA;AACA,GAfA;AAgBAC;AACAC,qBADA,+BACA;AAAA;;AACAC;AACA;AACAC,oBADA;AAEAC;AAFA;AAIAC,6BACAC,YADA,CACAC,WADA,EAEAC,IAFA,CAEA;AACA;AACA,OAJA,EAKAC,KALA,CAKA;AACAC;AACA,OAPA;AAQA,KAfA;AAgBAC,oBAhBA,8BAgBA;AACAT;AACA;AACAU;AACAC;AACA,OAFA;AAGA,KAtBA;AAuBAC,WAvBA,qBAuBA;AACAZ;AACA;AACA;AACAa;AACA;AACAC,0BACAC,iBADA,EAEA,CAFA,EAGA,CAHA,EAIAA,gBAJA,EAKAA,iBALA;AAOA,wCACAC,SADA,CACA,YADA,EAEAC,OAFA,CAEA,YAFA,EAEA,oBAFA;AAGAjB;AACAe,wCAbA,CAcA;AACA,OAfA,EAeAG,aAfA;AAgBA,KA3CA;AA4CAC,qBA5CA,6BA4CAC,OA5CA,EA4CA;AACApB;AACA;AACA;AACAqB,oBADA;AAEAC,0BAFA;AAGAC,+BAHA;AAIAC,gCAJA;AAKAC;AALA;AAOA,KAtDA;AAuDAC,iBAvDA,2BAuDA;AACA;;AACA;AACAC;AACA;;AACA;AACA,KA7DA;AA8DAC,SA9DA,mBA8DA;AACA;AACA;AACA;AAjEA;AAhBA","names":["name","components","VuePictureSwipe","data","isCameraOpen","canvasHeight","canvasWidth","items","mounted","methods","startCameraStream","console","audio","video","navigator","getUserMedia","constraints","then","catch","alert","stopCameraStream","tracks","track","capture","setTimeout","context","self","toDataURL","replace","FLASH_TIMEOUT","addToPhotoGallery","dataURI","src","thumbnail","w","h","alt","returnAndSave","Vue","close"],"sourceRoot":"src/views","sources":["cameraView.vue"],"sourcesContent":["<template>\n  <div class=\"camera-box\">\n    <b-container class=\"header\">\n      <b-row>\n        <b-col class=\"mx-0 px-0\" cols=\"1\">\n          <b-button class=\"ml-0 close\" variant=\"outline-secondary\" @click=\"close()\">\n            <b-icon icon=\"arrow-left\" />\n          </b-button>\n        </b-col>\n        <b-col class=\"px-0\" offset=\"8\">\n          <b-button class=\"upload\" v-on:click=\"returnAndSave()\" variant=\"success\">\n            Upload\n          </b-button>\n        </b-col>\n      </b-row>\n    </b-container>\n\n    <b-container class=\"take-photo\">\n      <b-row>\n        <b-col class=\"yourCamera\" sm=\"12\">\n          <div v-if=\"isCameraOpen\" class=\"camera-canvas\">\n            <video\n              ref=\"camera\"\n              :width=\"canvasWidth\"\n              :height=\"canvasHeight\"\n              autoplay\n            ></video>\n            <canvas\n              v-show=\"false\"\n              id=\"photoTaken\"\n              ref=\"canvas\"\n              :width=\"canvasWidth\"\n              :height=\"canvasHeight\"\n            ></canvas>\n          </div>\n        </b-col>\n        <b-col class=\"captureBtn\" sm=\"12\">\n          <b-button pill class=\"capturesave\" variant=\"outline-success\" @click=\"capture\">\n            <b-icon-camera-fill /> Capture\n          </b-button>\n        </b-col>\n        <b-col class=\"photoCaptured\" sm=\"12\">\n          <vue-picture-swipe :items=\"items\" class=\"pictureswipe\"></vue-picture-swipe>\n        </b-col>\n      </b-row>\n    </b-container>\n\n  </div>\n</template>\n\n<script>\nimport VuePictureSwipe from \"vue-picture-swipe\";\nimport Vue from \"vue\";\n\nexport default {\n  name: \"CameraApi\",\n  components: {\n    VuePictureSwipe,\n  },\n  data() {\n    return {\n      isCameraOpen: true,\n      canvasHeight: 300,\n      canvasWidth: 450,\n      items: [],\n    };\n  },\n  mounted() {\n    this.startCameraStream();\n  },\n  methods: {\n    startCameraStream() {\n      console.log(\"call start\");\n      const constraints = (window.constraints = {\n        audio: false,\n        video: true,\n      });\n      navigator.mediaDevices\n        .getUserMedia(constraints)\n        .then((stream) => {\n          this.$refs.camera.srcObject = stream;\n        })\n        .catch((error) => {\n          alert(\"Browser doesn't support or there is some errors.\" + error);\n        });\n    },\n    stopCameraStream() {\n      console.log(\"call stop\");\n      let tracks = this.$refs.camera.srcObject.getTracks();\n      tracks.forEach((track) => {\n        track.stop();\n      });\n    },\n    capture() {\n      console.log(\"call capture!\");\n      const FLASH_TIMEOUT = 50;\n      let self = this;\n      setTimeout(() => {\n        const context = self.$refs.canvas.getContext(\"2d\");\n        context.drawImage(\n          self.$refs.camera,\n          0,\n          0,\n          self.canvasWidth,\n          self.canvasHeight\n        );\n        const dataUrl = self.$refs.canvas\n          .toDataURL(\"image/jpeg\")\n          .replace(\"image/jpeg\", \"image/octet-stream\");\n        console.log(dataUrl);\n        self.addToPhotoGallery(dataUrl);\n        // self.uploadPhoto(dataUrl);\n      }, FLASH_TIMEOUT);\n    },\n    addToPhotoGallery(dataURI) {\n      console.log(\"add to photo gallery\");\n      this.items = [];\n      this.items.push({\n        src: dataURI,\n        thumbnail: dataURI,\n        w: this.canvasWidth / 2,\n        h: this.canvasHeight / 2,\n        alt: \"\",\n      });\n    },\n    returnAndSave() {\n      this.stopCameraStream();\n      if (this.items.length != 0) {\n        Vue.prototype.$photo = this.items[0].src;\n      }\n      this.$router.push(\"/journal/new\");\n    },\n    close() {\n      this.stopCameraStream();\n      this.$router.push(\"/journal/new\");\n    },\n  },\n};\n</script>\n\n<style scoped>\n@media (min-width:600px){\n  .camera-box {\n    margin-left: 200px;\n  }\n}\n\n@media only screen and (max-width:600px) {\n  .camera-box{\n    width: 100%;\n    min-width: 360px;\n    margin-bottom:100px;\n  }\n}\n\n/* Responsive Styling for Large screen */\n@media only screen and (min-width: 768px){\n  .upload {\n    position: absolute;\n    right: 25px;\n  }\n  .close {\n    margin-left: 5px;\n  }\n}\n\n/* Responsive Styling for Small screen */\n@media only screen and (max-width: 768px){\n  .upload {\n    position: absolute;\n    right: 10px;\n  }\n  .close {\n    margin-left: 5px;\n  }\n}\n\n.camera-box {\n  padding: 2px;\n  min-height: 600px;\n}\n.header {\n  margin-top: 20px;\n}\n.take-photo {\n  min-width: 300px;\n  height: 250px;\n  display: flex;\n  justify-content: center;\n  margin-top: 15px;\n}\n.pictureswipe {\n  height: 250px;\n  margin-top: 20px;\n}\n\n</style>\n"]},"metadata":{},"sourceType":"module"}